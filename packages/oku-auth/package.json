{
  "name": "@oku/auth",
  "version": "0.0.2",
  "private": false,
  "license": "MIT",
  "description": "General auth library, also supports capacitor storage",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"./**/*.{ts,vue}\"",
    "lint:fix": "eslint --fix \"./**/*.{ts,vue}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "@capacitor/storage": "^1.2.5",
    "crypto-js": "^4.1.1"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "jose": "^4.6.1",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@capacitor/storage": "^1.2.5",
    "@types/crypto-js": "^4.1.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "axios": "^0.27.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "tsup": "^6.0.0",
    "typescript": "^4.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
